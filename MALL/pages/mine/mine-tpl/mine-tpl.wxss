.mine-tpl-container {
  background: #fff;
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 15px;
  margin-bottom: 1px;
  /* border-bottom: 1px solid #f2f2f2; */
}

.mine-tpl-name {
  flex: 1;
  height: 80rpx;
  line-height: 80rpx;
  letter-spacing: 1px;
  text-align: left;
  font-size: 30rpx;
}

.mine-tpl-arrow {
  width: 8px;
  height: 8px;
  margin-right: 30px;
  border: 1px solid gray;
  transform: rotate(45deg);
  border-left-color: transparent;
  border-bottom-color: transparent;
}
